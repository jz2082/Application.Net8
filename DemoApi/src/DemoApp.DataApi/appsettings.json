{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppConsoleLogger": {
    "Environment": "#{environment}#",
    "LogLevel": "Information",
    "Color": "Yellow"
  },
  "InMemoryDbLogger": {
    "Environment": "#{environment}#",
    "LogLevel": "#{log-LogLevel}#",
    "ConnectionString": "#{log-dbConnection}#"
  },
  "PollyOptions": {
    "RetryCount": 3,
    "RetryDelayMilliSeconds": 1500,
    "NumberOfBreakEvent": 2,
    "DurationOfBreak": 30
  },
  "HealthCheckOptions": {
    "MemoryThreshold": 1073741824,
    "HttpCheckBaseAddress": "#{downStreamApiUrl}#",
    "HttpCheckEndPoint": "api/About/Health/DateTime",
    "SqlDbConnection": "",
    "InMemoryDbConnection": "#{dbConnection}#"
  },
  "Configuration": {
    "Environment": "#{environment}#",
    "DbConnection": "#{dbConnection}#",
    "DatabaseName": "#{dbName}#",
    "DownStreamApiUrl": "#{downStreamApiUrl}#",
    "CertificateFileName": "",
    "CertificatePassword": "",
    "AboutMessage": "#{aboutMessage}#"
  }
}