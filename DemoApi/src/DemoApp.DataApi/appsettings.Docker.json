{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppConsoleLogger": {
    "Environment": "Docker",
    "LogLevel": "Information",
    "Color": "Yellow"
  },
  "InMemoryDbLogger": {
    "Environment": "Docker",
    "LogLevel": "Information",
    "ConnectionString": "DemoDb"
  },
  "PollyOptions": {
    "RetryCount": 3,
    "RetryDelayMilliSeconds": 1500,
    "NumberOfBreakEvent": 2,
    "DurationOfBreak": 30
  },
  "HealthCheckOptions": {
    "MemoryThreshold": 1073741824,
    "HttpCheckBaseAddress": "https://net8-demodataapi:8081/",
    "HttpCheckEndPoint": "api/About/Health/DateTime",
    "SqlDbConnection": "",
    "InMemoryDbConnection": "DemoDb"
  },
  "Configuration": {
    "Environment": "Docker",
    "DbConnection": "DemoDb",
    "DatabaseName": "DemoDb",
    "DownStreamApiUrl": "https://net8-demodataapi:8081/",
    "CertificateFileName": "",
    "CertificatePassword": "",
    "AboutMessage": "Docker Configuration (Demo.DataApi)"
  }
}